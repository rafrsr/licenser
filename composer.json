{
  "name": "rafrsr/licenser",
  "description": "Adds license headers to your source files",
  "license": "MIT",
  "authors": [
    {
      "name": "Rafael SR",
      "email": "raf.rsr@gmail.com",
      "homepage": "https://github.com/rafrsr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rafrsr\\Licenser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rafrsr\\Licenser\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6",
    "symfony/console": "~2.8|~3.0",
    "symfony/finder": "~2.8|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1|^5.1",
    "satooshi/php-coveralls": "dev-master",
    "symfony/filesystem": "^3.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "bin": [
    "bin/licenser"
  ]
}
