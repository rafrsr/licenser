{
  "name": "rafrsr/licenser",
  "description": "Adds license headers to your source files",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Rafael SR",
      "email": "raf.rsr@gmail.com",
      "homepage": "https://github.com/rafrsr"
    }
  ],
  "keywords": [
    "licenser",
    "license",
    "docheader",
    "docblock",
    "header"
  ],
  "autoload": {
    "psr-4": {
      "Rafrsr\\Licenser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rafrsr\\Licenser\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6",
    "symfony/console": "~2.8|~3.0",
    "twig/twig": "~1.2",
    "symfony/finder": "~2.8|~3.0",
    "symfony/yaml": "~2.8|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "satooshi/php-coveralls": "dev-master",
    "symfony/filesystem": "^3.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "bin": [
    "bin/licenser"
  ]
}
